<script>
    // 點選左上角logo不跳轉
    $('a[href="/"]').click(function (e) {
        e.preventDefault();
        return;
    });

    // 漂浮按鈕
    $('.sizelinkbtn').click(function (e) {
        e.preventDefault();
        let innerheight = window.innerHeight;
        $('html , body').animate({
            // scrollTop: $('#sizelink').offset().top - (0.5 * innerheight),
            scrollTop: $('#sizelink').offset().top - 100,
        }, 900);
    });

    //網址帶有錨點的客人，三秒再推一把
    if (location.hash == "#summer_offer") {
        setTimeout(() => {
            $('html , body').animate({
                scrollTop: $('#summer_offer').offset().top,
            }, 900);
        }, 3000)
    }

    //copy折扣碼 
    function copybtn(couponcode) {
        // 複製（利用 Clipboard API 中的寫入功能）
        navigator.clipboard.writeText(couponcode);
        confirm(`恭喜您成功領取折扣碼！\n您已複製以下優惠代碼 : ${couponcode}！\n於購物車選擇商品後，於「優惠代碼」欄位輸入 即可享總結帳金額再折MAX6%`)
    }

    $(".copybtn").on("click", function () {
        copybtn("08LUNASS");
    });

    $('#summer_banner').click(function (e) {
        e.preventDefault();
        copybtn("08LUNASS");
        let innerheight = window.innerHeight;
        $('html , body').animate({
            // scrollTop: $('#sizelink').offset().top - (0.5 * innerheight),
            scrollTop: $('#summer_event').offset().top - 100,
        }, 900);
    });

    $('#summer_event').click(function (e) {
        e.preventDefault();
        copybtn("08LUNASS");
        let innerheight = window.innerHeight;
        $('html , body').animate({
            // scrollTop: $('#sizelink').offset().top - (0.5 * innerheight),
            scrollTop: $('#summer_offer').offset().top - 100,
        }, 900);
    });
</script>
