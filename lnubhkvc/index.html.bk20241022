<script>
    //coupon 專用複製折扣碼
    function coupon_copy() {
        navigator.clipboard.writeText("202408SS");
        confirm(`恭喜您成功複製轉盤折扣碼！\n請於購物車「優惠代碼」欄位輸入，\n即享最高會員優惠後再折HKD$24元\n優惠代碼：202408SS`)
    }

    $("#coupon_countdown").on("click", function () {
        coupon_copy();
    });

    let localCoupon = Date.parse(localStorage.getItem('localCoupon'));
    $("#popup_gif").on("click", function () {
        coupon_copy();
        $("#shopline_popup").hide();

        // 計算coupon倒數計時器的開始、結束時間
        const coupon_sec = 1000;
        const coupon_min = coupon_sec * 60;
        const coupon_start_time = new Date();  //popup視窗，確認複製完折扣碼後的那個時間點，計時開始start
        const coupon_end_time = new Date(coupon_start_time.getTime() + (coupon_min * 1)); //coupon_start_time加上？分鐘，計時結束end

        //寫入 coupon_end_time 到 localstorage
        localStorage.setItem('localCoupon', coupon_end_time);

        $("#coupon_countdown").show();
    });

    let now_time = new Date();
    let scroll_first = true;
    
    //判斷localStorage是否已經有存coupon_end_time
    if (!localCoupon) {
        window.onscroll = function () {
            if (scroll_first) {
                $("#shopline_popup").show();
            }
            scroll_first = false;
        };

        //每1秒刷新一次
        setInterval(() => {
            now_time = new Date();
            localCoupon = Date.parse(localStorage.getItem('localCoupon'));
            const differ_end = localCoupon - now_time.getTime();
            //判斷刷新後的now_time，是否還在期間內
            if (differ_end > 0) {
                coupon_timer(differ_end);
            } else {
                $("#coupon_countdown").hide();
            }
        }, 1000);

    } else {
        if (now_time < localCoupon) {
            $("#coupon_countdown").show();

            //每1秒刷新一次
            setInterval(() => {
                now_time = new Date();
                const differ_end = localCoupon - now_time.getTime();
                //判斷刷新後的now_time，是否還在期間內
                if (differ_end > 0) {
                    coupon_timer(differ_end);
                } else {
                    $("#coupon_countdown").hide();
                }
            }, 1000);
        } else {
            window.onscroll = function () {
                if (scroll_first) {
                    $("#shopline_popup").show();
                }
                scroll_first = false;
            };

            //每1秒刷新一次
            setInterval(() => {
                now_time = new Date();
                localCoupon = Date.parse(localStorage.getItem('localCoupon'));
                const differ_end = localCoupon - now_time.getTime();
                //判斷刷新後的now_time，是否還在期間內
                if (differ_end > 0) {
                    coupon_timer(differ_end);
                } else {
                    $("#coupon_countdown").hide();
                }
            }, 1000);
        }
    }

    function coupon_timer(differ_end) {
        const sec = Math.floor(differ_end / 1000 % 60);
        const min = Math.floor(differ_end / 1000 / 60 % 60);

        document.querySelector('#coupon_countdown .coupon_seconds_number').textContent = String(sec).padStart(2, '0');
        document.querySelector('#coupon_countdown .coupon_minutes_number').textContent = String(min).padStart(2, '0');
    }
</script>